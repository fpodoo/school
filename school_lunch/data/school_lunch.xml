<odoo>
    <data noupdate="1">

        <record id="product_meal" model="product.product">
            <field name="name">Lunch Meal</field>
            <field name="type">service</field>
            <field name="list_price">4.50</field>
        </record>

        <record id="product_soup" model="product.product">
            <field name="name">Soup</field>
            <field name="type">service</field>
            <field name="list_price">0.50</field>
        </record>

        <!-- Classes -->

        <record id="class_name_p1" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P1A</field>
        </record>
        <record id="class_name_p2" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P2A</field>
        </record>
        <record id="class_name_p3" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P3A</field>
        </record>
        <record id="class_name_p4" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P4A</field>
        </record>
        <record id="class_name_p5" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P5A</field>
        </record>
        <record id="class_name_p6" model="school_lunch.class_name">
            <field name="class_type">1</field>
            <field name="name">P6A</field>
        </record>
        <record id="class_name_m1" model="school_lunch.class_name">
            <field name="class_type">0</field>
            <field name="name">M1</field>
        </record>
        <record id="class_name_m2" model="school_lunch.class_name">
            <field name="class_type">0</field>
            <field name="name">M2</field>
        </record>
        <record id="class_name_m3" model="school_lunch.class_name">
            <field name="class_type">0</field>
            <field name="name">M3</field>
        </record>

        <!-- Guests -->

        <record id="class_name_guest" model="school_lunch.class_name">
            <field name="class_type">3</field>
            <field name="name">Invité</field>
        </record>
        <record id="kid_guest" model="school_lunch.kid">
            <field name="firstname">Invité</field>
            <field name="lastname">Externe</field>
            <field name="class_id" ref="school_lunch.class_name_guest"/>
        </record>
    </data>
</odoo>
